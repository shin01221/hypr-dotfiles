'/home/shin/.zen/u66syh3c.Default (alpha)/chrome/userContent.css'
@media {

  /* Common variables affecting all pages */
  @-moz-document url-prefix("about:") {
    :root {
      --in-content-page-color: #<wallbash_txt1> !important;
      --color-accent-primary: #<wallbash_1xa5> !important;
      --color-accent-primary-hover: rgb(<wallbash_pry2_rgb>) !important;
      --color-accent-primary-active: rgb(<wallbash_2xa2_rgb>) !important;
      --background-color: #<wallbash_1xa5> !important;
      --in-content-page-background: #<wallbash_pry1> !important;
    }

  }

  /* Variables and styles specific to about:newtab and about:home */
  @-moz-document url("about:newtab"), url("about:home") {

    :root {
      --newtab-background-color: #<wallbash_pry1> !important;
      --newtab-background-color-secondary: #<wallbash_pry2> !important;
      --newtab-element-hover-color: #<wallbash_pry2> !important;
      --newtab-text-primary-color: #<wallbash_txt1> !important;
      --newtab-wordmark-color: #<wallbash_txt1> !important;
      --newtab-primary-action-background: #<wallbash_1xa5> !important;
    }

    .icon {
      color: #<wallbash_1xa5> !important;
    }

    .search-wrapper .logo-and-wordmark .logo {
      background: url("https://raw.githubusercontent.com/dim-ghub/ZenBash/refs/heads/main/hyde.png") no-repeat center !important;
      display: inline-block !important;
      height: 82px !important;
      width: 82px !important;
      background-size: 82px !important;
    }

    @media (max-width: 609px) {
      .search-wrapper .logo-and-wordmark .logo {
        background-size: 64px !important;
        height: 64px !important;
        width: 64px !important;
      }
    }

    .card-outer:is(:hover, :focus, .active):not(.placeholder) .card-title {
      color: #<wallbash_1xa5> !important;
    }

    .top-site-outer .search-topsite {
      background-color: #<wallbash_1xa5> !important;
    }

    .compact-cards .card-outer .card-context .card-context-icon.icon-download {
      fill: #a6e3a1 !important;
    }
  }

  /* Variables and styles specific to about:preferences */
  @-moz-document url-prefix("about:preferences") {
    :root {
      --zen-colors-tertiary: #<wallbash_pry1> !important;
      --in-content-text-color: #<wallbash_txt1> !important;
      --link-color: #<wallbash_1xa5> !important;
      --link-color-hover: rgb(<wallbash_1xa7_rgb>) !important;
      --zen-colors-primary: #<wallbash_pry2> !important;
      --in-content-box-background: #<wallbash_pry2> !important;
      --zen-primary-color: #<wallbash_1xa5> !important;
    }

    groupbox , moz-card{
      background: #<wallbash_pry1> !important;
    }

    button,
    groupbox menulist {
      background: #<wallbash_pry2> !important;
      color: #<wallbash_txt1> !important;
    }

    .main-content {
      background-color: #<wallbash_pry1> !important;
    }

    .identity-color-blue {
      --identity-tab-color: #8aadf4 !important;
      --identity-icon-color: #8aadf4 !important;
    }

    .identity-color-turquoise {
      --identity-tab-color: #8bd5ca !important;
      --identity-icon-color: #8bd5ca !important;
    }

    .identity-color-green {
      --identity-tab-color: #a6da95 !important;
      --identity-icon-color: #a6da95 !important;
    }

    .identity-color-yellow {
      --identity-tab-color: #eed49f !important;
      --identity-icon-color: #eed49f !important;
    }

    .identity-color-orange {
      --identity-tab-color: #f5a97f !important;
      --identity-icon-color: #f5a97f !important;
    }

    .identity-color-red {
      --identity-tab-color: #ed8796 !important;
      --identity-icon-color: #ed8796 !important;
    }

    .identity-color-pink {
      --identity-tab-color: #f5bde6 !important;
      --identity-icon-color: #f5bde6 !important;
    }

    .identity-color-purple {
      --identity-tab-color: #c6a0f6 !important;
      --identity-icon-color: #c6a0f6 !important;
    }
  }

  /* Variables and styles specific to about:addons */
  @-moz-document url-prefix("about:addons") {
    :root {
      --zen-dark-color-mix-base: #<wallbash_pry1> !important;
      --background-color-box: #<wallbash_pry1> !important;
    }
  }

  /* Variables and styles specific to about:protections */
  @-moz-document url-prefix("about:protections") {
    :root {
      --zen-primary-color: #<wallbash_pry1> !important;
      --social-color: #cba6f7 !important;
      --coockie-color: #89dceb !important;
      --fingerprinter-color: #f9e2af !important;
      --cryptominer-color: #b4befe !important;
      --tracker-color: #a6e3a1 !important;
      --in-content-primary-button-background-hover: rgb(<wallbash_pry2_rgb) !important;
      --in-content-primary-button-text-color-hover: #<wallbash_txt1> !important;
      --in-content-primary-button-background: #<wallbash_pry2> !important;
      --in-content-primary-button-text-color: #<wallbash_txt1> !important;
    }


    .card {
      background-color: #<wallbash_pry2> !important;
    }
  }
}
